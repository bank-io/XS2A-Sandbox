{
  "name": "tpp-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port=4205 --proxy-config proxy.conf.json",
    "start:proxy:dev": "ng serve --port=4205 --proxy-config=proxy.dev.conf.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "test": "ng test",
    "test-ci": "ng test --browsers ChromeHeadless --watch=false --environment=prod",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier --write",
    "prettier-check": "find . -name '*.js' -or -name '*.ts' -or -name '*.html' -or -name '*.css' -or -name '*.scss'| grep -v build | grep -v \"assets/lib\" | grep -v node_modules | grep -v dist | xargs prettier-check"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.14",
    "@angular/cdk": "^8.1.3",
    "@angular/common": "^8.2.14",
    "@angular/compiler": "^8.2.14",
    "@angular/core": "^8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "^8.2.14",
    "@angular/platform-browser-dynamic": "^8.2.14",
    "@angular/router": "^8.2.14",
    "@auth0/angular-jwt": "^2.1.0",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "bootstrap": "^4.3.1",
    "core-js": "^2.5.4",
    "css-vars-ponyfill": "^2.2.0",
    "custom-event-polyfill": "^1.0.7",
    "font-awesome": "^4.7.0",
    "jasmine": "^3.5.0",
    "jszip": "^3.2.0",
    "material-icons": "^0.3.1",
    "ng-http-loader": "^6.0.1",
    "ng2-file-upload": "^1.3.0",
    "ngx-filter-pipe": "^2.1.2",
    "node-sass": "^4.13.1",
    "rxjs": "^6.5.4",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jquery": "^3.4.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage-istanbul-reporter": "^2.0.5",
    "karma-jasmine": "^1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "popper.js": "^1.15.0",
    "protractor": "^6.0.0",
    "ts-node": "~5.0.1",
    "typescript": "~3.5.3",
    "codelyzer": "^5.2.2",
    "pretty-quick": "^2.0.1",
    "prettier-check": "^2.0.0",
    "prettier": "^2.0.4",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-angular": "^3.0.2",
    "husky": "^4.2.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged && ng lint"
    }
  }
}
